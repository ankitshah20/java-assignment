<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.halo.carInfoWithAi">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/aaa"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.HighRiskPeopleActivity"
            android:exported="true" />
        <activity android:name=".Activity.EditProfileActivity" />
        <activity android:name=".Activity.ProfileActivity" />
        <activity android:name=".Activity.AddCovidPatientActivity" />
        <activity android:name=".Activity.CovidPatientListActivity" />
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.LowRIskPeopleActivity" />
        <activity android:name=".Activity.CovidPatientDetailActivity" />
        <activity android:name=".Activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.SignUpActivity" />
        <activity android:name=".Activity.LoginActivity" />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Base.Theme.AppCompat" />
    </application>

</manifest>